pipeline
{ 
  agent any
  stages
  {
    stage("Build")
    {
     steps
     {
       checkout scm
     }
   }
   stage("static_analysis")
   {
      steps
      {
        echo "$names"
        sh label: '', script: 'sh shell.sh $names'
        sh label: '', script: 'curl -X POST -d @result.json https://devopsiotdemo.service-now.com/api/x_snc_devops_iot/get_static_analysis_report_from_device/capture?scanId=$scanID&branchID=$branchID --header "Content-Type:application/json"'
      }
   }
  }
}
